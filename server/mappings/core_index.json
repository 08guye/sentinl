{
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 1
  },
  "mappings": {
    "watch": {
      "properties": {
        "input": {
          "type": "object",
          "enabled": false
        },
        "action": {
          "type": "object",
          "enabled": false
        },
        "transform": {
          "type": "object",
          "enabled": false
        },
        "condition": {
          "type": "object",
          "enabled": false
        },
        "uuid": {
          "type":  "string",
          "index": "not_analyzed"
        },
        "report": {
          "type": "boolean"
        },
        "disable": {
          "type": "boolean"
        }
      }
    }
  }
}
